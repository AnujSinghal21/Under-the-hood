    %vpi_call 2 16 "$monitor", "A = %b, B = %b, C = %b", v000001c3bb2fbde0_0, v000001c3bb2fbe80_0, v000001c3bb2fbf20_0 {0 0 0};
    %pushi/vec4 20, 0, 32;
T_4.0 %dup/vec4;
